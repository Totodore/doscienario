{
  "name": "doscienario",
  "version": "2.0.45",
  "description": "Application de gestion de projets de jeux vidéos",
  "main": "dist/main.js",
  "author": {
    "name": "Théodore Prévot",
    "email": "prevottheodore@gmail.com",
    "url": "https://github.com/totodore"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "build": "ng build --prod --optimization=false --base-href ./",
    "build:electron": "tsc --project tsconfig.electron.json",
    "build:linux": "electron-builder build -l --publish=never",
    "build:windows": "electron-builder build -w --publish=never",
    "format:imports": "tslint --config tslint-imports.json --fix --project .",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/color-picker": "^5.0.2",
    "@angular/animations": "~11.0.1",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "~11.0.1",
    "@angular/compiler": "~11.0.1",
    "@angular/core": "~11.0.1",
    "@angular/forms": "~11.0.1",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "~11.0.1",
    "@angular/platform-browser-dynamic": "~11.0.1",
    "@angular/router": "~11.0.1",
    "@ckeditor/ckeditor5-angular": "^2.0.1",
    "axios": "^0.24.0",
    "custom-electron-titlebar": "^3.2.7",
    "diff": "^5.0.0",
    "electron-updater": "^4.3.9",
    "fast_array_intersect": "^1.1.0",
    "fast-array-diff": "^1.0.0",
    "fs-extra": "^9.1.0",
    "ngx-indexed-db": "^9.4.2",
    "ngx-logger": "^5.0.3",
    "ngx-socket-io": "^2.1.1",
    "rxjs": "~6.6.0",
    "typescript": "4.0.2",
    "uuid": "^8.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.4.1",
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.1",
    "@types/diff": "^5.0.0",
    "@types/electron": "^1.6.10",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "^1.4.35",
    "@types/uuid": "^8.3.0",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^6.0.0",
    "electron": "^12.0.0",
    "electron-builder": "^22.10.5",
    "ts-node": "~8.3.0",
    "tsc-watch": "^4.2.9",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "tslint-etc": "^1.13.10",
    "typescript": "~4.0.2"
  },
  "build": {
    "appId": "fr.scriptis.doscienario",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "icons/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "icons/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Office",
      "icon": "icons/icon.png"
    }
  }
}
