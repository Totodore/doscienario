{
  "name": "doscienario",
  "version": "2.1.1",
  "description": "Application de gestion de projets créatifs",
  "main": "dist/main.js",
  "author": {
    "name": "Théodore Prévot",
    "email": "prevottheodore@gmail.com",
    "url": "https://github.com/totodore"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "build": "ng build --configuration production --optimization=false --base-href ./",
    "build:prod": "yarn build",
    "build:int": "ng build --configuration integration --optimization=false --base-href ./",
    "format:imports": "tslint --config tslint-imports.json --fix --project .",
    "proto:generate:win": "for /f %G in ('dir /b ..\\doscenario-services\\proto\\*.proto') do npx grpc_tools_node_protoc --plugin=protoc-gen-ng=.\\node_modules\\.bin\\protoc-gen-ng.cmd --ng_out=.\\src\\proto -I ..\\doscenario-services\\proto ..\\doscenario-services\\proto\\%G",
    "proto:generate": "protoc --plugin=protoc-gen-ng=$(which protoc-gen-ng) --ng_out=<OUTPUT_PATH> -I <PROTO_DIR_PATH> <PROTO_FILES>",
    "lint": "ng lint",
    "tauri": "tauri"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/color-picker": "^8.0.0",
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@ckeditor/ckeditor5-angular": "^4.0.0",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@ngx-grpc/common": "^3.1.1",
    "@ngx-grpc/core": "^3.1.1",
    "@ngx-grpc/improbable-eng-grpc-web-client": "^3.1.1",
    "@ngx-grpc/well-known-types": "^3.1.1",
    "@tauri-apps/api": "^1.2.0",
    "axios": "^0.27.2",
    "crc": "^4.1.1",
    "diff": "^5.1.0",
    "fast-array-diff": "^1.0.1",
    "fast_array_intersect": "^1.1.0",
    "fs-extra": "^10.1.0",
    "google-protobuf": "^3.21.2",
    "ngx-indexed-db": "^11.0.2",
    "ngx-logger": "5.0.11",
    "ngx-socket-io": "^4.3.0",
    "rxjs": "~7.5.6",
    "transformation-matrix": "^2.12.0",
    "typescript": "4.8.2",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.1",
    "@angular-devkit/build-angular": "^14.2.1",
    "@angular/cli": "^14.2.1",
    "@angular/compiler-cli": "^14.2.0",
    "@ngx-grpc/protoc-gen-ng": "^3.1.1",
    "@tauri-apps/cli": "^1.2.3",
    "@types/diff": "^5.0.2",
    "@types/google-protobuf": "^3.15.6",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.4",
    "angular-cli-ghpages": "^1.0.3",
    "codelyzer": "^6.0.2",
    "grpc-tools": "^1.12.4",
    "reflect-metadata": "^0.1.13",
    "ts-node": "~10.9.1",
    "tsc-watch": "^5.0.3",
    "tslib": "^2.4.0",
    "tslint": "^6.1.3",
    "tslint-etc": "^1.13.10"
  },
  "packageManager": "yarn@3.2.3"
}
